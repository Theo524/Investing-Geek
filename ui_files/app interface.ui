                            <item alignment="Qt::AlignHCenter">
                             <widget class="QFrame" name="wha_is_a_stock">
                              <property name="minimumSize">
                               <size>
                                <width>0</width>
                                <height>0</height>
                               </size>
                              </property>
                              <property name="frameShape">
                               <enum>QFrame::StyledPanel</enum>
